<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>01_semi</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>  
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>contentPage</servlet-name>
  	<jsp-file>/WEB-INF/views/travel/contentPage.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>contentPage</servlet-name>
  	<url-pattern>/travel/contentPage</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>roomReservation</servlet-name>
  	<jsp-file>/WEB-INF/views/travel/roomReservation.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>roomReservation</servlet-name>
  	<url-pattern>/travel/roomReservation</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>roomContent</servlet-name>
  	<jsp-file>/WEB-INF/views/travel/roomContent.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>roomContent</servlet-name>
  	<url-pattern>/travel/roomContent</url-pattern>
  </servlet-mapping>
</web-app>